{
  "name": "web-wizardry",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"tailwindcss --input ./app/globals.css --output ./app/output.css --watch\"",
    "tw:watch": "tailwindcss --input ./app/globals.css --output ./app/output.css --watch",
    "build": "tailwindcss ./app/globals.css --output ./app/output.css && next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install"
  },
  "dependencies": {
    "@next/font": "13.1.1",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "concurrently": "^7.6.0",
    "eslint": "8.30.0",
    "eslint-config-next": "13.1.1",
    "next": "13.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "showdown": "^2.1.0",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@types/showdown": "^2.0.0",
    "autoprefixer": "^10.4.13",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "tailwindcss": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,css,md}": "prettier --write"
  }
}
