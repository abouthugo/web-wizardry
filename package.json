{
  "name": "web-wizardry",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"next dev\" \"tailwindcss --input ./app/globals.css --output ./app/output.css --watch\"",
    "dev:init": "npm run quit-nginx && npm run init-ssl && npm run init-nginx",
    "predev": "npm run dev:init --hostname=arclocal.abouthugo.dev",
    "tw:watch": "tailwindcss --input ./app/globals.css --output ./app/output.css --watch",
    "build": "tailwindcss ./app/globals.css --output ./app/output.css && next build",
    "start": "next start",
    "-------------------------------[utils]-------------------------------": "",
    "create-env": "printenv > .env.production",
    "lint": "next lint",
    "prepare": "husky install",
    "-------------------------------[ssl & nginx]-------------------------------": "",
    "init-ssl": "./scripts/init_ssl.sh",
    "init-nginx": "./nginx/init-nginx.sh",
    "quit-nginx": "./nginx/quit-nginx.sh",
    "-------------------------------[format]-------------------------------": ""
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.0.2",
    "@react-hookz/web": "^24.0.4",
    "@supabase/supabase-js": "^2.21.0",
    "@tanstack/react-query": "^5.37.1",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "class-variance-authority": "^0.7.0",
    "classnames": "^2.5.1",
    "clsx": "^2.1.0",
    "concurrently": "^7.6.0",
    "eslint": "8.30.0",
    "gray-matter": "^4.0.3",
    "lucide-react": "^0.359.0",
    "luxon": "^3.4.4",
    "next": "^14.1.3",
    "react": "^18.2.0",
    "react-div-100vh": "^0.7.0",
    "react-dom": "^18.2.0",
    "react-fast-marquee": "^1.6.4",
    "remark": "^14.0.3",
    "remark-html": "^15.0.2",
    "showdown": "^2.1.0",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.35.6",
    "@types/luxon": "^3.4.2",
    "@types/showdown": "^2.0.0",
    "autoprefixer": "^10.4.13",
    "eslint-config-next": "^14.1.3",
    "husky": "^8.0.2",
    "lint-staged": "^13.1.0",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "supabase": "^1.50.12",
    "tailwindcss": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,css,md}": "prettier --write"
  }
}
